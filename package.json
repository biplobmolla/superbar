{
	"name": "superbar",
	"version": "0.0.1",
	"description": "Command bar for the web",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"watch:tailwind": "cross-env NODE_ENV=development postcss src/assets/css/tailwind.css -o dist/tailwind.dist.css -w",
		"dev:tailwind": "cross-env NODE_ENV=development postcss src/assets/css/tailwind.css -o dist/tailwind.dist.css",
		"build:tailwind": "cross-env NODE_ENV=production postcss src/assets/css/tailwind.css -o dist/tailwind.dist.css",
		"build": "npm run build:tailwind && webpack",
		"build:staging": "npm run build:tailwind && webpack --env staging",
		"start": "concurrently \"npm run watch:tailwind\" \"webpack --env development --watch\""
	},
